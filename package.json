    "@as-integrations/fastify": "^2.1.1",
    "@fastify/cors": "^8.5.0",
    "@fastify/static": "^7.0.0",
    "@graphql-tools/schema": "^10.0.23",
    "axios": "^1.9.0",
    "fastify": "^4.26.2",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.4",
    "react-router-dom": "^7.5.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.1",
